<cover-view 
bindtap="backBtn_callback"   
style="position:absolute;margin-left: 10px;margin-top: {{menuButtonTop}}px; margin-bottom:0px;height:{{menuButtonHeight}}px;line-height:{{menuButtonHeight}}px;color:white;">
<cover-image style="width:{{menuButtonHeight}}px;height:{{menuButtonHeight}}px;" src="/assets/back.png"></cover-image>
</cover-view>

<canvas
        id="canvas1"
        class="canvas1"
        type="webgl"
        disable-scroll = "true"
        bindtouchend="bindtouchend_callback">
        <cover-view class="cover-view">
            <cover-view>测量结果：{{measureResult}}米</cover-view>
        </cover-view>         
</canvas>

